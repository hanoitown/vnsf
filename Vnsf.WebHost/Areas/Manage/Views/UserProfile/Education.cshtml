@model IEnumerable<Vnsf.WebHost.Areas.Cheetah.Models.EducationViewModel>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Specialization)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProgramDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EducationPlace)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Duration)
        </th>
